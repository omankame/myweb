pipeline{
    agent any
    environment{
        PATH = "/opt/maven/bin:$PATH"
    }
    stages{
        stage('Git Checkout'){
            steps{
                git 'https://github.com/omankame/myweb'
            }
            
        }
        
        stage('Maven Build'){
            steps{
                sh "maven clean package"
            }
        }
    }
}
